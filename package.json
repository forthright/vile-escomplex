{
  "name": "@forthright/vile-escomplex",
  "version": "0.1.3",
  "description": "A vile plugin for tracking complexity via escomplex",
  "license": "MPL-2.0",
  "repository": "https://github.com/forthright/vile-escomplex",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "keywords": [
    "escomplex",
    "complexity",
    "javascript",
    "maintainability",
    "vile"
  ],
  "scripts": {
    "coffee": "coffee",
    "compile": "node bin/compile",
    "compile-dev": "coffee -cbo .test test",
    "dev": "bin/dev",
    "istanbul": "istanbul",
    "release-it": "release-it",
    "test": "_mocha --recursive -u bdd -R spec -t 2000 .test/spec.js",
    "test-cov": "node bin/test-cov",
    "lint-cov": "node bin/lint-cov",
    "update": "ncu -a",
    "vile": "vile p -usi -x src:lib,test.coffee:.test.js --nodecorations"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "escomplex": "^2.0.0-alpha",
    "lodash": "^4.17.4",
    "unixify": "^0.2.1"
  },
  "devDependencies": {
    "@forthright/vile": ">=0.11.8",
    "@forthright/vile-comment": ">=0.1.17",
    "@forthright/vile-coverage": ">=0.2.14",
    "@forthright/vile-depcheck": ">=0.1.12",
    "@forthright/vile-eclint": ">=0.1.10",
    "@forthright/vile-escomplex": "^0.1.2",
    "@forthright/vile-eslint": ">=0.1.13",
    "@forthright/vile-git": "^0.5.1",
    "@forthright/vile-language": ">=0.1.12",
    "@forthright/vile-ncu": ">=0.2.1",
    "@forthright/vile-newline": ">=0.1.12",
    "@forthright/vile-nsp": ">=0.2.12",
    "@forthright/vile-semver": ">=0.1.11",
    "@forthright/vile-stat": "0.0.14",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "coffee-script": "~1.12.3",
    "constable": "~0.0.4",
    "istanbul": "~0.4.5",
    "mimus": "~0.2.3",
    "mocha": "~3.2.0",
    "npm-check-updates": ">=2.8.9",
    "release-it": "^2.5.3",
    "shelljs": "^0.7.6",
    "sinon": "~1.17.7",
    "sinon-chai": "~2.8.0"
  },
  "peerDependencies": {
    "@forthright/vile": ">=0.11.8"
  }
}
